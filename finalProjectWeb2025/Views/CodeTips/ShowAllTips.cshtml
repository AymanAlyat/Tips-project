@model  List<Tips>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<table class="table">
    <tr>
        <th>Name</th>
        <th>Title</th>
        <th>Content</th>
        <th>Category</th>
        <th></th>
        
     
    </tr>
    @foreach (var tip in Model)
    {
        <tr>
            <td>@tip.User?.Name</td>
            <td>@tip.Title</td>
            <td>@tip.Content</td>
            <td>@tip.Category</td>
            <td><a href="/CodeTips/Delete/@tip.TipId">Delete</a></td> <!--رابط لما اكبس عليه باعتبار ناديت على الاكشن ميثود المختصه بالحذف-->
        </tr>
    }
</table>
